<script>
(function () {
  const PER_PAGE = 10;
  let currentPage = 1;

  const jsonEl = document.getElementById('bv-jsonld-reviews-data');
  if (!jsonEl) {
    console.error('[reviews] JSON tidak ditemukan');
    return;
  }

  const data = JSON.parse(jsonEl.textContent);
  const reviews = data.review || [];

  const listEl = document.getElementById('reviews-list');
  const countEl = document.getElementById('reviews-count');
  const prevBtn = document.getElementById('prevPage');
  const nextBtn = document.getElementById('nextPage');

  if (!listEl) {
    console.error('[reviews] #reviews-list tidak ditemukan');
    return;
  }

  function renderPage() {
    listEl.innerHTML = ''; //  CLEAR DOM TOTAL

    const total = reviews.length;
    const start = (currentPage - 1) * PER_PAGE;
    const end = Math.min(start + PER_PAGE, total);

    //  render HANYA data page ini
    reviews.slice(start, end).forEach(r => {
      const stars = ''.slice(
        5 - r.reviewRating.ratingValue,
        10 - r.reviewRating.ratingValue
      );

      listEl.insertAdjacentHTML('beforeend', `
        <div class="review-item">
          <div class="review-left">
            <div class="review-author">${r.author.name}</div>
          </div>

          <div class="review-right">
            <div class="review-rating">
              <div class="review-stars">${stars}</div>
              <div class="review-title">${r.headline}</div>
            </div>

            <div class="review-meta">
              <span class="review-author-name">${r.author.name}</span>
              <span class="review-date">
                ${new Date(r.datePublished).toLocaleDateString('id-ID')}
              </span>
            </div>

            <div class="review-body">${r.reviewBody}</div>
            <a href="#" class="review-translate">
                                Terjemahkan dengan Google
                                </a><div class="review-badges">
                                <span class="review-badge">
                                    <img
                                    src="https://photos-eu.bazaarvoice.com/photo/2/cGhvdG86YXR0cmlidXRpb25sb2dvMg/0d15984f-2a9e-483a-a644-5f44404db9a5"
                                    alt="Nike"
                                    />
                                    Di-posting pertama kali di Nike
                                </span>
                                </div>
          </div>
        </div>
      `);
    });

    // count text
    if (countEl) {
      countEl.textContent = `${start + 1}  ${end} dari ${total} Ulasan`;
    }

    // button state
    if (prevBtn) prevBtn.disabled = currentPage === 1;
    if (nextBtn) nextBtn.disabled = end >= total;
  }

  if (prevBtn) {
    prevBtn.onclick = () => {
      if (currentPage > 1) {
        currentPage--;
        renderPage();
      }
    };
  }

  if (nextBtn) {
    nextBtn.onclick = () => {
      if (currentPage * PER_PAGE < reviews.length) {
        currentPage++;
        renderPage();
      }
    };
  }

  // INIT
  renderPage();
})();
</script>
