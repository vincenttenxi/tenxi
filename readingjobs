                    <script type="text/javascript">
                      require(["jquery", "mage/mage"], function ($) {
                        $(document).ready(function () {
                          var $multistore = $(".weltpixel_multistore");

                          // Toggle open on click
                          $multistore
                            .find(".current-store")
                            .on("click", function (e) {
                              e.stopPropagation(); // Prevent bubbling to document
                              $multistore.toggleClass("open");
                              $(
                                "#search_bar_container .unbxd-as-wrapper.unbxd-as-overall-autosuggest",
                              ).hide();
                            });

                          // Close when clicking outside
                          $(document).on("click", function (e) {
                            if (
                              !$multistore.is(e.target) &&
                              $multistore.has(e.target).length === 0
                            ) {
                              $multistore.removeClass("open");
                            }
                          });
                        });
                      });
                    </script>
