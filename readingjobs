    <script>
(function () {
  function initTabs() {
    const titles = document.querySelectorAll('.data.item.title[role="tab"]');

    titles.forEach(title => {
      title.addEventListener('click', function () {
        const targetId = this.getAttribute('aria-controls');
        if (!targetId) return;

        // reset all tabs
        titles.forEach(t => {
          t.classList.remove('active');
          t.setAttribute('aria-selected', 'false');
          t.setAttribute('aria-expanded', 'false');
        });

        document.querySelectorAll('.data.item.content[role="tabpanel"]').forEach(c => {
          c.style.display = 'none';
          c.setAttribute('aria-hidden', 'true');
        });

        // activate clicked tab
        this.classList.add('active');
        this.setAttribute('aria-selected', 'true');
        this.setAttribute('aria-expanded', 'true');

        const content = document.getElementById(targetId);
        if (content) {
          content.style.display = '';
          content.setAttribute('aria-hidden', 'false');
        }

        // kalau BV ada, refresh
        if (window.BV && BV.ui && BV.ui.refresh) {
          BV.ui.refresh();
        }
      });
    });
  }

  document.addEventListener('DOMContentLoaded', initTabs);
})();
</script>

    <script>
        (function () {
        const DESKTOP_MIN = 768;
        const TOP_OFFSET = 160; // header height (bisa kamu adjust)

        function initStickyGallery() {
            if (window.innerWidth < DESKTOP_MIN) return;

            const gallery = document.querySelector('.sticky-gallery');
            const media = document.querySelector('.product.media');

            if (!gallery || !media) return;

            // NETRALISIR efek JS lama (kalau ada)
            media.style.height = 'auto';
            gallery.style.position = 'sticky';
            gallery.style.top = TOP_OFFSET + 'px';
            gallery.style.width = 'auto';

            // safety
            gallery.classList.add('sticky-by-custom-js');
        }

        // init
        document.addEventListener('DOMContentLoaded', initStickyGallery);
        window.addEventListener('resize', initStickyGallery);
        })();
        </script>

