                  <script>
                    require(["jquery", "jquery.drilldown"], function ($) {
                      // $('.price-box.price-final_price', $("#nav17685419036969cecf04b7f")).each(function(){
                      //     $(this).removeClass('price-box').addClass('price-box1');
                      //     $(this).attr('data-role','priceBox1');
                      // })
                      $(".price-box", $("#nav17685419036969cecf04b7f")).each(
                        function () {
                          $(this)
                            .removeClass("price-box")
                            .addClass("price-box1");
                          $(this).attr("data-role", "priceBox1");
                        },
                      );

                      jQuery(document).ready(function ($) {
                        jQuery("#nav17685419036969cecf04b7f p").each(
                          function () {
                            var $this = $(this);
                            if (
                              $this.html().replace(/\s|&nbsp;/g, "").length == 0
                            )
                              $this.remove();
                          },
                        );

                        $("#nav17685419036969cecf04b7f .nav-item").hover(
                          function () {
                            if ($(window).width() > 800) {
                              $(this).addClass("current");
                              var id = $(this).data("dynamic-id");
                              if (id) {
                                $(this)
                                  .parents(".dynamic-items")
                                  .find("li")
                                  .removeClass("dynamic-active");
                                $(this).addClass("dynamic-active");
                                $("#nav17685419036969cecf04b7f ." + id)
                                  .parent()
                                  .find(".dynamic-item")
                                  .removeClass("dynamic-active");
                                $(
                                  "#nav17685419036969cecf04b7f ." + id,
                                ).addClass("dynamic-active");
                              }

                              if ($(this).data("hovericon")) {
                                $(this)
                                  .children(".nav-anchor")
                                  .find(".item-icon")
                                  .attr("src", $(this).data("hovericon"));
                              }
                              if (
                                $(this).data("caret") &&
                                $(this).data("hovercaret")
                              ) {
                                $(this)
                                  .children(".nav-anchor")
                                  .find(".ves-caret")
                                  .removeClass($(this).data("caret"))
                                  .addClass($(this).data("hovercaret"));
                              }

                              var child_anchor =
                                $(this).children(".nav-anchor");
                              $(child_anchor).css({
                                "background-color":
                                  $(child_anchor).data("hover-bgcolor"),
                                color: $(child_anchor).data("hover-color"),
                              });
                              if ($(this).hasClass("level0")) {
                                var mParentTop = $(
                                  "#top-menu-1768541903 .navigation",
                                ).offset().top;
                                var mParentHeight = $(this).parent().height();
                                var mTop = $(this).height();
                                var mHeight = $(this).height();
                                var mParent = $(this).parent();
                                if (mHeight < mParentHeight) {
                                  mTop =
                                    $(this).offset().top -
                                    mParent.offset().top +
                                    mHeight;
                                }
                                $(this).children(".submenu").css({ top: mTop });
                              }
                            }
                          },
                          function () {
                            if ($(window).width() > 800) {
                              var id = $(this).attr("id");
                              if ($(this).data("iconsrc")) {
                                $(this)
                                  .children(".nav-anchor")
                                  .find(".item-icon")
                                  .attr("src", $(this).data("iconsrc"));
                              }
                              if ($(this).data("caret")) {
                                $(this)
                                  .children(".nav-anchor")
                                  .find(".ves-caret")
                                  .removeClass($(this).data("hovercaret"))
                                  .addClass($(this).data("caret"));
                              }
                              $(this).removeClass("current");
                              var child_anchor =
                                $(this).children(".nav-anchor");
                              var link_bgcolor =
                                $(child_anchor).data("bgcolor");
                              var link_color = $(child_anchor).data("color");
                              if (
                                !link_bgcolor ||
                                typeof link_bgcolor == "undefined"
                              ) {
                                link_bgcolor = "none";
                              }
                              if (
                                !link_color ||
                                typeof link_color == "undefined"
                              ) {
                                link_color = "none";
                              }

                              $(child_anchor).css({
                                "background-color": link_bgcolor,
                                color: link_color,
                              });
                            }
                          },
                        );

                        /** ACCORDION */

                        /* OPENER */
                        $("#nav17685419036969cecf04b7f .opener").on(
                          "click",
                          function (e) {
                            e.preventDefault();
                            $(
                              "#nav17685419036969cecf04b7f .nav-item",
                            ).removeClass("item-active");
                            var parent = $(this).parents(".nav-item").eq(0);
                            $(this).toggleClass("item-active");
                            var subMenu = $(parent).find(".submenu").eq(0);
                            subMenu.stop().slideToggle();
                            subMenu.css({ height: "" });
                            return false;
                          },
                        );

                        /* TOGGLE */
                        $(
                          "#top-menu-1768541903 .ves-navtoggletop-menu-1768541903",
                        ).click(function () {
                          $("html").removeClass("nav-before-open nav-open");
                          $(".ves-overlaytop-menu-1768541903").show();
                          $("#top-menu-1768541903").append(
                            '<div class="ves-overlay ves-overlaytop-menu-1768541903"></div>',
                          );
                          $("#nav17685419036969cecf04b7f").css("left", "0px");
                          if ($("html").hasClass("ves-navopen")) {
                            $("html").removeClass("ves-navopen");
                            setTimeout(function () {
                              $("html").removeClass("ves-nav-before-open");
                            }, 300);
                          } else {
                            $("html").addClass("ves-nav-before-open");
                            setTimeout(function () {
                              $("html").addClass("ves-navopen");
                            }, 42);
                          }
                        });

                        /* OFF-CANVAS */
                        $(document).on(
                          "click",
                          ".ves-overlaytop-menu-1768541903",
                          function () {
                            $("#nav17685419036969cecf04b7f").css("left", "");
                            $("html").removeClass("ves-navopen");
                            setTimeout(function () {
                              $("html").removeClass("ves-nav-before-open");
                            }, 300);
                            $(this).remove();
                            return false;
                          },
                        );

                        /* STICKY MENY */

                        var mobileDrill, desktopDrill;
                        /* WINDOW RESIZE */
                        $(window)
                          .on("resize", function () {
                            var disable_bellow_number = $(
                              "#nav17685419036969cecf04b7f",
                            ).attr("data-disable-bellow");
                            if (disable_bellow_number) {
                              if (
                                $(window).width() <
                                parseInt(disable_bellow_number)
                              ) {
                                $("#top-menu-1768541903").hide();
                              } else {
                                $("#top-menu-1768541903").show();
                              }
                            }
                            if ($(window).width() < 801) {
                              $("#top-menu-1768541903")
                                .addClass("nav-mobile")
                                .removeClass("nav-desktop");
                              /** DRILLDOWN MOBILE MENU */

                              $("#top-menu-1768541903 .drill-opener").show();
                              mobileDrill = $("#top-menu-1768541903").drilldown(
                                {
                                  selector: ".drill-opener",
                                  cssClass: {
                                    container:
                                      "navitaion17685419036969cecf04b7f",
                                    root: "nav17685419036969cecf04b7f",
                                    sub: "submenu",
                                    back: "drilldown-back",
                                  },
                                  speed: 300,
                                },
                              );
                              $("#top-menu-1768541903 .opener").removeClass(
                                "item-active",
                              );
                            } else {
                              $("#top-menu-1768541903 .submenu").css({
                                display: "",
                              });
                              $("#top-menu-1768541903")
                                .removeClass("nav-mobile")
                                .addClass("nav-desktop");

                              /** DRILLDOWN MOBILE MENU */
                              $("#top-menu-1768541903 .drill-opener").hide();
                              $("#top-menu-1768541903").css("width", "");
                              if (typeof mobileDrill === "object") {
                                $("#top-menu-1768541903").drilldown("reset");
                              }

                              $("#top-menu-1768541903")
                                .find(".submenu")
                                .each(function () {
                                  var subWidth = $(this).data("width");
                                  if (!subWidth) {
                                    subWidth = "100%";
                                  }
                                  $(this).css("width", subWidth);
                                });
                            }

                            // TABLET
                            $("#nav17685419036969cecf04b7f .nav-item").click(
                              function () {
                                if (
                                  $(window).width() >= 800 &&
                                  $(window).width() < 1024
                                ) {
                                  var parent = $(this).parent();
                                  if (
                                    $(parent).children(".submenu").length == 1
                                  ) {
                                    if ($(parent).hasClass("level0")) {
                                      if ($(parent).hasClass("current")) {
                                        $(parent).removeClass("current");
                                        return false;
                                      }
                                      $(
                                        "#nav17685419036969cecf04b7f .level0",
                                      ).removeClass("current");
                                      var mParent = $(
                                        "#top-menu-1768541903 .navigation",
                                      ).offset().top;
                                      var mHeight = $(parent).offset().top;
                                      var mTop =
                                        mHeight - mParent + $(parent).height();
                                      $(parent)
                                        .children(".submenu")
                                        .css({ top: mTop });
                                    }
                                    $(parent).addClass("current");
                                    if (
                                      $(parent).hasClass("submenu-alignleft") ||
                                      $(parent).hasClass("submenu-alignright")
                                    ) {
                                      if (
                                        $(parent).offset().left +
                                          $(parent)
                                            .children(".submenu")
                                            .width() >
                                        $(window).width()
                                      ) {
                                        $(parent)
                                          .children(".submenu")
                                          .css("max-width", "100%");
                                        $(parent).css("position", "static");
                                      }
                                    }
                                    return false;
                                  }
                                } else {
                                  $("#nav17685419036969cecf04b7f")
                                    .find(".submenu")
                                    .css("max-width", "");
                                  $("#nav17685419036969cecf04b7f")
                                    .find(".submenu-alignleft")
                                    .css("position", "relative");
                                }
                              },
                            );
                          })
                          .resize();
                      });
                    });
                  </script>

